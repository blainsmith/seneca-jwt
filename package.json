{
  "name": "seneca-jwt",
  "version": "2.0.0",
  "description": "Seneca plugin to work with JSON Web Tokens.",
  "main": "jwt.js",
  "scripts": {
    "test": "npm run lint && npm run tape",
    "lint": "xo",
    "tape": "tape 'test/**/*_test.js' | tap-spec",
    "coverage": "istanbul cover tape test/**/*.js | tap-spec",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blainsmith/seneca-jwt.git"
  },
  "keywords": [
    "jwt",
    "seneca",
    "microservices"
  ],
  "author": "Blain Smith <rebelgeek@blainsmith.com> (http://blainsmith.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blainsmith/seneca-jwt/issues"
  },
  "homepage": "https://github.com/blainsmith/seneca-jwt#readme",
  "devDependencies": {
    "coveralls": "^2.11.6",
    "istanbul": "^0.4.1",
    "seneca": "^0.9.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.2.2",
    "xo": "^0.12.0"
  },
  "dependencies": {
    "jsonwebtoken": "^5.4.1",
    "random-string": "^0.1.2"
  }
}
